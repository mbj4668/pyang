<?xml version="1.0" encoding="UTF-8"?>
<module name="yt2"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:yt2="urn:ietf:params:xml:ns:yang:yt2"
        xmlns:yt1="urn:ietf:params:xml:ns:yang:yt1">
  <namespace uri="urn:ietf:params:xml:ns:yang:yt2"/>
  <prefix value="yt2"/>
  <import module="yt1g">
    <prefix value="yt1"/>
  </import>
  <organization>
    <text>YANG Language Design Team</text>
  </organization>
  <contact>
    <text>Andy Bierman</text>
  </contact>
  <description>
    <text>YANG test module 2.</text>
  </description>
  <revision date="2007-11-26">
    <description>
      <text>Initial revision.</text>
    </description>
  </revision>
  <typedef name="Str1">
    <type name="string">
      <length value="1..10 | 40 | 52">
        <error-message>
          <value>Wrong Str1 length</value>
        </error-message>
      </length>
      <pattern value="[a-z][A-Z]*">
        <error-message>
          <value>Wrong Str1 pattern</value>
        </error-message>
        <error-app-tag value="str1-violation"/>
      </pattern>
      <pattern value=".*xx">
        <error-message>
          <value>Wrong Str1 pattern</value>
        </error-message>
        <error-app-tag value="str1-violation"/>
      </pattern>
    </type>
  </typedef>
  <typedef name="Union2">
    <type name="union">
      <type name="yt1:Num6">
        <range value="1"/>
      </type>
      <type name="yt1:Num3"/>
      <type name="enumeration">
        <enum name="one">
          <description>
            <text>first enum</text>
          </description>
          <status value="current"/>
        </enum>
        <enum name="two">
          <value value="7"/>
        </enum>
      </type>
      <type name="Union1"/>
      <type name="union">
        <type name="int32"/>
        <type name="string"/>
      </type>
    </type>
  </typedef>
  <typedef name="Union1">
    <type name="union">
      <type name="yt1:Num1"/>
      <type name="uint32"/>
      <type name="int32">
        <range value="min..-1"/>
      </type>
      <type name="string">
        <length value="0..255"/>
      </type>
    </type>
  </typedef>
  <leaf name="dec">
    <type name="decimal64">
      <fraction-digits value="1"/>
      <range value="-0.3..0.0"/>
    </type>
  </leaf>
</module>
