<?xml version="1.0" encoding="UTF-8"?>
<module name="lt1"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:lt1="http://example.com/lt1">
  <namespace uri="http://example.com/lt1"/>
  <prefix value="lt1"/>
  <grouping name="foo">
    <list name="feuillage">
      <description>
        <text>Local feuillage docstring</text>
      </description>
      <uses name="bar">
        <refine target-node="blatt">
          <default value="beech"/>
        </refine>
      </uses>
      <uses name="baz">
        <refine target-node="leaves">
          <default value="cz"/>
        </refine>
        <refine target-node="leaves/cz/lupen">
          <min-elements value="0"/>
        </refine>
        <refine target-node="leaves/dustbin/dustbin/hoja">
          <description>
            <text>Refined description of hoja</text>
          </description>
        </refine>
      </uses>
      <key value="feuille"/>
    </list>
    <anyxml name="rubbish"/>
  </grouping>
  <typedef name="trees">
    <type name="enumeration">
      <enum name="oak"/>
      <enum name="beech"/>
      <enum name="birch"/>
      <enum name="elm"/>
      <enum name="linden"/>
    </type>
    <default value="oak"/>
  </typedef>
  <grouping name="bar">
    <leaf name="blatt">
      <default value="linden"/>
      <type name="trees"/>
    </leaf>
    <leaf name="feuille">
      <type name="int8"/>
      <must condition=". &gt; 40"/>
    </leaf>
  </grouping>
  <grouping name="baz">
    <choice name="leaves">
      <case name="cz">
        <leaf-list name="lupen">
          <description>
            <text>Base desc. of lupen</text>
          </description>
          <type name="string"/>
          <max-elements value="3"/>
        </leaf-list>
      </case>
      <list name="dustbin">
        <key value="hoja"/>
        <leaf name="hoja">
          <description>
            <text>Base desc. of hoja</text>
          </description>
          <type name="string"/>
        </leaf>
      </list>
    </choice>
  </grouping>
  <uses name="foo">
    <augment target-node="feuillage">
      <leaf name="foo">
        <description>
          <text>augmented foo description</text>
        </description>
        <type name="string"/>
      </leaf>
    </augment>
    <refine target-node="feuillage/feuille">
      <must condition=". &lt;= 100"/>
    </refine>
    <refine target-node="feuillage/blatt">
      <default value="birch"/>
    </refine>
    <refine target-node="feuillage/leaves/lt1:cz/lupen">
      <description>
        <text>Refined desc. of lupen</text>
      </description>
      <max-elements value="2"/>
    </refine>
    <refine target-node="feuillage">
      <min-elements value="1"/>
      <reference>
        <text>feuillage reference</text>
      </reference>
    </refine>
  </uses>
</module>
