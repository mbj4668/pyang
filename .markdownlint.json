{
  "code-fence-style": {
    "style": "backtick"
  },
  "fenced-code-language": {
    "allowed_languages": [
      "sh",
      "json",
      "lisp",
      "ini",
      "text"
    ],
    "language_only": true
  },
  "heading-style": {
    "style": "atx"
  },
  "hr-style": {
    "style": "---"
  },
  "line-length": {
    "strict": true,
    "code_blocks": false
  },
  "link-image-style": {
    "collapsed": false,
    "shortcut": false,
    "url_inline": false
  },
  "no-duplicate-heading": {
    "siblings_only": true
  },
  "ol-prefix": {
    "style": "ordered"
  },
  "reference-links-images": {
    "shortcut_syntax": true
  },
  "strong-style": {
    "style": "asterisk"
  },
  "MD013": {
    "line_length": 160
  }
}
